{
  "name": "markdown-render",
  "version": "1.0.0",
  "description": "A web project that accepts Markdown scripts and renders them as a displayable page.",
  "main": "src/index.tsx",
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -b gh-pages -d build"
  },
  "dependencies": {
    "marked": "^2.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "react-split": "^2.0.14",
    "react-split-pane": "^0.1.92"
  },
  "devDependencies": {
    "@types/marked": "^5.0.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "gh-pages": "^6.3.0",
    "typescript": "^4.1.3"
  },
  "homepage": "https://grant-huang.github.io/markdown-render",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
